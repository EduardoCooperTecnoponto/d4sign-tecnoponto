{
    "name": "tecnoponto/d4sign",
    "description": "Pacote de integração com a D4sign para a Tecnoponto",
    "license": "MIT",
    "authors": [
        {
            "name": "Tecnoponto",
            "email": "edmar_espinola@hotmail.com"
        },
        {
            "name": "Eduardo M. Cooper",
            "email": "oeduardocooper@gmail.com"
        }        
    ],
    "require": {
        "guzzlehttp/guzzle": "*",
        "illuminate/support": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.7",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Tecnoponto\\D4sign\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tecnoponto\\D4sign\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tecnoponto\\D4sign\\Providers\\D4SignServiceProvider"
            ]
        }
    }
}
